{
    "table": "Otps",
    "database": "application",
    "attributes": {
        "id": {
            "type": "INTEGER",
            "primaryKey": true,
            "autoIncrement": true
        },
        "otp": {
            "type": "STRING",
            "defaultValue": null
        },
        "type": {
            "type": "STRING",
            "defaultValue": "sms"
        },
        "extraInfo": {
            "type": "JSONB",
            "defaultValue": null
        },
        "isActive": {
            "type": "BOOLEAN",
            "defaultValue": true
        },
        "_status": {
            "type": "STRING"
        },
        "deletedAt": {
            "type": "DATE",
            "allowNull": true
        }
    },
    "associations": [
        {
            "data": [
                {
                    "data": {
                        "as": "Owner",
                        "sourceKey": "id",
                        "foreignKey": "createdBy"
                    },
                    "type": "belongsTo"
                },
                {
                    "data": {
                        "as": "Updater",
                        "sourceKey": "id",
                        "foreignKey": "updatedBy"
                    },
                    "type": "belongsTo"
                },
                {
                    "data": {
                        "as": "Destroyer",
                        "sourceKey": "id",
                        "foreignKey": "deletedBy"
                    },
                    "type": "belongsTo"
                },
                {
                    "type": "belongsTo",
                    "data": {
                        "foreignKey": "userId",
                        "as": "User",
                        "sourceKey": "id"
                    }
                }
            ],
            "model": "Users"
        },
        {
            "model": "MailComms",
            "data": [
                {
                    "type": "belongsTo",
                    "data": {
                        "foreignKey": "mailCommId",
                        "sourceKey": "id"
                    }
                }
            ]
        },
        {
            "model": "MailComms",
            "data": [
                {
                    "type": "belongsTo",
                    "data": {
                        "foreignKey": "smsCommId",
                        "sourceKey": "id"
                    }
                }
            ]
        }
    ]
}